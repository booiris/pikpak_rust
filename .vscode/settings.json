{
    "files.exclude": {
        "Cargo.lock": true,
        "**/node_modules": true,
        "**/pnpm-lock.yaml": true,
        "LICENSE": true,
        "target": true,
        "openapitools.json": true,
        "website/src/api": true,
    },
    "cSpell.words": [
        "ahash",
        "Avenir",
        "chrono",
        "consts",
        "Encrypter",
        "fontawesome",
        "fortawesome",
        "humantime",
        "jsonwebtoken",
        "mypikpak",
        "nightwatch",
        "openapi",
        "openapitools",
        "pikcloud",
        "pikpak",
        "pinia",
        "reqwest",
        "rushstack",
        "thiserror",
        "tsbuildinfo",
        "ttps",
        "utoipa",
        "Vite"
    ],
    "resmon.show.mem": true,
    "resmon.show.disk": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "code-runner.runInTerminal": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.executorMap": {
        "rust": "cd $workspaceRoot && cargo run server"
    },
    "code-runner.saveFileBeforeRun": true,
    "code-runner.preserveFocus": false,
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "rust-analyzer.check.features": [
        "__local_test"
    ],
    "rust-analyzer.cargo.features": [
        "__local_test"
    ],
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "**/consts.rs"
    ],
}